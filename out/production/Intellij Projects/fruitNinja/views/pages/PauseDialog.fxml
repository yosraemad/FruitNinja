<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<GridPane xmlns="http://javafx.com/javafx" alignment="CENTER"
          xmlns:fx="http://javafx.com/fxml"
          prefHeight="400.0" prefWidth="600.0">
    <Button text="Resume" onAction="#resumeBtnClicked" GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <Button text="Restart" onAction="#restartBtnClicked" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
    <Button fx:id="quitBtn" text="Quit Game" onAction="#quitBtnClicked" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
    <stylesheets>
        <URL value="@../css/PauseDialogStyleSheet.css"/>
    </stylesheets>
</GridPane>
