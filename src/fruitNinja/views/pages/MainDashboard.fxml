<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane style="-fx-background-color: #381c0f;" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fitHeight="800.0" fitWidth="1300.0" layoutX="-12.0" layoutY="-12.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/MaindashboardBackground.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="151.0" fitWidth="276.0" layoutX="482.0" layoutY="33.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/fruit-ninja-logo.png" />
            </image>
        </ImageView>
        <ProgressBar fx:id="levelBar" layoutX="149.0" layoutY="105.0" prefHeight="18.0" prefWidth="191.0" progress="0.48" style="-fx-accent: Purple; -fx-background-color: burlywood;" />
        <Label fx:id="levelNum" layoutX="188.0" layoutY="60.0" prefHeight="51.0" prefWidth="118.0" style="-fx-font-family: 'Gang of Three'; -fx-text-fill: White; -fx-font-size: 35px; -fx-font-weight: Bold;" text="Level " textAlignment="CENTER" >
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label fx:id="level" layoutX="109.0" layoutY="86.0" prefHeight="51.0" prefWidth="27.0" style="-fx-font-family: 'Gang of Three'; -fx-text-fill: white; -fx-font-size: 35px; -fx-font-weight: Bold;"  />
        <Button fx:id="newGameButton" layoutX="508.0" layoutY="259.0" mnemonicParsing="false" prefHeight="210.0" prefWidth="224.0" style="-fx-background-color: transparent;">
            <graphic>
                <ImageView fitHeight="301.0" fitWidth="282.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/playGame.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="leaderBoardButton" layoutX="79.0" layoutY="469.0" mnemonicParsing="false" style="-fx-background-color: transparent;" styleClass="focused">
            <graphic>
                <ImageView fitHeight="188.0" fitWidth="211.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/LeaderBoardButton.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="backButton" layoutX="994.0" layoutY="469.0" mnemonicParsing="false" style="-fx-background-color: transparent;">
            <graphic>
                <ImageView fitHeight="219.0" fitWidth="240.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/BackButton.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
      <Label fx:id="playerName" layoutX="340.0" layoutY="601.0" prefHeight="36.0" prefWidth="170.0" style="-fx-rotate: 25; -fx-text-fill: White; -fx-font-weight: Bold; -fx-font-family: 'Gang of Three'; -fx-font-size: 18;" />
    </children>
</AnchorPane>
